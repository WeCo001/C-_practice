class NumberManipulator
{
    public void getValue(out int x)
    {
        int temp =5;
        x = temp;
    }
    
    static void Main(string[] args)
    {
        NumberManipulator n = new NumberManipulator();
        
        int a = 100;
        Console.WriteLine("在调用方法之前a的值: {0}", a);
        
        n.getValue(out a);
        Console.WriteLine("在调用方法之后a的值: {0}", a);
        Console.ReadLine();
    }
}


结果：
100
5
