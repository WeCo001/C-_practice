static void Main(string[] args)
{
    hanoi(3, 'A', 'B', 'C');
}

static void Move(char from, char to)
{
    Console.WriteLine("From {0} to {1}.", form, to);
}

static void hanoi(int n, char p1, char p2, char p3)
{
    if (n == 1)
    {
        Move(p1, p3);
    }
    else()
    {
        hanoi(n - 1, p1, p3, p2);
        Move(p1, p3);
        hanoi(n - 1, p2, p1, p3);
    }
}
